{
  "name": "frontend",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/frontend",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/frontend",
        "outputPath": "dist/apps/frontend"
      },
      "configurations": {
        "development": {
          "outputPath": "apps/frontend/.next"
        },
        "production": {}
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "development",
      "options": {
        "command": "next dev --port 4200",
        "cwd": "apps/frontend"
      },
      "configurations": {
        "development": {
          "command": "next dev --port 4200",
          "cwd": "apps/frontend"
        },
        "production": {
          "command": "next start --port 4200",
          "cwd": "apps/frontend"
        }
      }
    },
    "start": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "production",
      "options": {
        "buildTarget": "frontend:build:production",
        "port": 4200
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/frontend/jest.config.js",
        "passWithNoTests": false
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/frontend/**/*.{ts,tsx,js,jsx}"],
        "outputFile": "dist/apps/frontend/lint-results.json",
        "eslintConfig": "apps/frontend/.eslintrc.json",
        "ignorePath": "apps/frontend/.eslintignore"
      }
    }
  },
  "tags": []
}
