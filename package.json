{
  "name": "@nestjs-backend/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --target=build --projects=backend,frontend",
    "build:backend": "nx build backend",
    "build:frontend": "nx build frontend",
    "build:prod": "nx run-many --target=build --projects=backend,frontend --configuration=production",
    "build:staging": "nx build backend --configuration=staging",
    "test": "nx run-many --target=test --projects=backend,frontend",
    "test:backend": "nx test backend",
    "test:frontend": "nx test frontend",
    "test:watch": "nx test backend --watch",
    "test:cov": "nx run-many --target=test --projects=backend,frontend --coverage",
    "test:e2e": "nx e2e backend-e2e",
    "test:integration": "nx test backend --testPathPattern=integration",
    "test:unit": "nx test backend --testPathPattern=spec",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "lint": "nx run-many --target=lint --projects=backend,frontend",
    "lint:backend": "nx lint backend",
    "lint:frontend": "nx lint frontend",
    "lint:fix": "nx run-many --target=lint --projects=backend,frontend --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "start": "nx run-many --target=serve --projects=backend,frontend",
    "start:backend": "nx serve backend",
    "start:frontend": "nx serve frontend",
    "start:dev": "nx run-many --target=serve --projects=backend,frontend --watch",
    "start:debug": "nx serve backend --inspect",
    "start:prod": "node dist/apps/backend/main.js",
    "dev": "nx run-many --target=serve --projects=backend,frontend",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "db:seed:prod": "node -r ts-node/register prisma/seed.ts",
    "docker:build": "docker build -t nestjs-backend .",
    "docker:run": "docker run -p 3000:3000 nestjs-backend",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f app",
    "deploy:staging": "npm run build:staging && npm run db:migrate:deploy",
    "deploy:prod": "npm run build:prod && npm run db:migrate:deploy",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1",
    "prepare": "husky install",
    "postinstall": "prisma generate"
  },
