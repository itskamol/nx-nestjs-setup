{
  "name": "@nestjs-backend/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --target=build --projects=backend",
    "build:backend": "nx build backend",
    "build:prod": "nx run-many --target=build --projects=backend --configuration=production",
    "build:staging": "nx build backend --configuration=staging",
    "test": "nx run-many --target=test --projects=backend",
    "test:backend": "nx test backend",
    "test:watch": "nx test backend --watch",
    "test:cov": "nx run-many --target=test --projects=backend --coverage",
    "test:e2e": "nx e2e backend-e2e",
    "test:integration": "nx test backend --testPathPattern=integration",
    "test:unit": "nx test backend --testPathPattern=spec",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "lint": "nx run-many --target=lint --all",
    "lint:backend": "nx lint backend",
    "lint:shared": "nx lint shared",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "start": "nx serve backend --watch",
    "dev": "nx run-many --target=serve --projects=backend",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "db:seed:prod": "node -r ts-node/register prisma/seed.ts",
    "docker:build": "docker build -t nestjs-backend .",
    "docker:run": "docker run -p 3000:3000 nestjs-backend",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f app",
    "deploy:staging": "npm run build:staging && npm run db:migrate:deploy",
    "deploy:prod": "npm run build:prod && npm run db:migrate:deploy",
    "prepare": "husky install",
    "postinstall": "prisma generate"
  },
  "private": true,
  "dependencies": {
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.6",
    "@prisma/client": "^6.13.0",
    "@types/express": "^5.0.3",
    "@types/uuid": "^10.0.0",
    "axios": "latest",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "express": "latest",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "multer": "^2.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "react": "^19.1.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "simple-xml-to-json": "^1.2.3",
    "socket.io": "^4.8.1",
    "tslib": "^2.8.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "xmlbuilder2": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@eslint/js": "^9.32.0",
    "@nestjs/testing": "^11.1.6",
    "@nx/eslint-plugin": "21.3.11",
    "@nx/jest": "21.3.11",
    "@nx/node": "21.3.11",
    "@nx/webpack": "21.3.11",
    "@swc-node/register": "~1.10.10",
    "@swc/core": "~1.13.3",
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "babel-jest": "^30.0.5",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jest-util": "^30.0.5",
    "nx": "21.3.11",
    "prettier": "^3.6.2",
    "prisma": "^6.13.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  }
}
