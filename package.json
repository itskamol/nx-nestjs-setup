{
  "name": "@nestjs-backend/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx build backend",
    "build:prod": "nx build backend --configuration=production",
    "build:staging": "nx build backend --configuration=staging",
    "test": "nx test backend",
    "test:watch": "nx test backend --watch",
    "test:cov": "nx test backend --coverage",
    "test:e2e": "nx e2e backend-e2e",
    "test:integration": "nx test backend --testPathPattern=integration",
    "test:unit": "nx test backend --testPathPattern=spec",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "lint": "nx lint backend",
    "lint:fix": "nx lint backend --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "start": "nx serve backend",
    "start:dev": "nx serve backend --watch",
    "start:debug": "nx serve backend --inspect",
    "start:prod": "node dist/apps/backend/main.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "db:seed:prod": "node -r ts-node/register prisma/seed.ts",
    "docker:build": "docker build -t nestjs-backend .",
    "docker:run": "docker run -p 3000:3000 nestjs-backend",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f app",
    "deploy:staging": "npm run build:staging && npm run db:migrate:deploy",
    "deploy:prod": "npm run build:prod && npm run db:migrate:deploy",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1",
    "prepare": "husky install",
    "postinstall": "prisma generate"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^11.1.5",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.5",
    "@nestjs/event-emitter": "^3.0.0",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.5",
    "@nestjs/platform-socket.io": "^11.1.5",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.0.5",
    "@prisma/client": "^6.13.0",
    "@types/express": "^5.0.3",
    "@types/uuid": "^10.0.0",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "hikvision-isapi-client": "^0.1.52",
    "ioredis": "^5.7.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.5",
    "@nx/eslint": "21.3.11",
    "@nx/eslint-plugin": "21.3.11",
    "@nx/jest": "21.3.11",
    "@nx/js": "21.3.11",
    "@nx/nest": "21.3.11",
    "@nx/node": "21.3.11",
    "@nx/web": "21.3.11",
    "@nx/webpack": "21.3.11",
    "@nx/workspace": "21.3.11",
    "@swc-node/register": "~1.10.10",
    "@swc/core": "~1.13.3",
    "@swc/helpers": "~0.5.17",
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "~24.2.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/socket.io": "^3.0.2",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "jest-environment-node": "^30.0.5",
    "jest-util": "^30.0.5",
    "jsonc-eslint-parser": "^2.4.0",
    "nx": "21.3.11",
    "prettier": "^3.6.2",
    "prisma": "^6.13.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "10.9.2",
    "tslib": "^2.8.1",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.39.0",
    "webpack-cli": "^6.0.1"
  }
}
