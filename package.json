{
  "name": "@nestjs-backend/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx build backend",
    "build:prod": "nx build backend --configuration=production",
    "build:staging": "nx build backend --configuration=staging",
    "test": "nx test backend",
    "test:watch": "nx test backend --watch",
    "test:cov": "nx test backend --coverage",
    "test:e2e": "nx e2e backend-e2e",
    "test:integration": "nx test backend --testPathPattern=integration",
    "test:unit": "nx test backend --testPathPattern=spec",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "lint": "nx lint backend",
    "lint:fix": "nx lint backend --fix",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "start": "nx serve backend",
    "start:dev": "nx serve backend --watch",
    "start:debug": "nx serve backend --inspect",
    "start:prod": "node dist/apps/backend/main.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:reset": "prisma migrate reset",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "db:seed:prod": "node -r ts-node/register prisma/seed.ts",
    "docker:build": "docker build -t nestjs-backend .",
    "docker:run": "docker run -p 3000:3000 nestjs-backend",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f app",
    "deploy:staging": "npm run build:staging && npm run db:migrate:deploy",
    "deploy:prod": "npm run build:prod && npm run db:migrate:deploy",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1",
    "prepare": "husky install",
    "postinstall": "prisma generate"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^11.0.0",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/throttler": "^6.2.1",
    "@prisma/client": "^5.7.1",
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^11.1.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@nx/eslint": "21.3.11",
    "@nx/eslint-plugin": "21.3.11",
    "@nx/jest": "21.3.11",
    "@nx/js": "21.3.11",
    "@nx/nest": "21.3.11",
    "@nx/node": "21.3.11",
    "@nx/web": "21.3.11",
    "@nx/webpack": "21.3.11",
    "@nx/workspace": "21.3.11",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^30.0.0",
    "@types/node": "~18.16.9",
    "@types/passport-jwt": "^4.0.0",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "husky": "^8.0.3",
    "jest": "^30.0.2",
    "jest-environment-node": "^30.0.2",
    "jest-util": "^30.0.2",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "21.3.11",
    "prettier": "^2.6.2",
    "prisma": "^5.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.8.2",
    "typescript-eslint": "^8.29.0",
    "webpack-cli": "^5.1.4"
  }
}
